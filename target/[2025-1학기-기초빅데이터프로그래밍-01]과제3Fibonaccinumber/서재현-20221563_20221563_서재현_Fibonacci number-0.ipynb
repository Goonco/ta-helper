    "%%writefile fibonacci.py\n",
    "num = int(input(\"input:\"))\n",
    "\n",
    "def fibo(x):\n",
    "    if x == 0:\n",
    "        return 0\n",
    "    elif x == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibo(x - 1) + fibo(x - 2)\n",
    "\n",
    "print(fibo(num))"
   ]
