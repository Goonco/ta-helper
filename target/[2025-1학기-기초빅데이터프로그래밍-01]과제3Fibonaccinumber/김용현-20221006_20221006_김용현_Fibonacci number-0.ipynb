    "%%writefile fibonacci.py\n",
    "def fibo(x):\n",
    "    if x>2:\n",
    "        return fibo(x-1) + fibo(x-2)\n",
    "    else:\n",
    "        return 1\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    N = int(input(\"input: \"))\n",
    "    print(fibo(N))"
