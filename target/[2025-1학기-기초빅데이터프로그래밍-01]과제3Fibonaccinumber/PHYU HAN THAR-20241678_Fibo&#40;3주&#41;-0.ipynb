    "def fibo(n):\n",
    "    if n <= 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        a, b = 0, 1\n",
    "        for _ in range(2, n + 1):\n",
    "            a, b = b, a + b\n",
    "        return b\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    n = int(input(\"input: \"))\n",
    "    print(fibo(n))\n"
   ]
